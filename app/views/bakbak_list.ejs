<% include layouts/headOpen %>
<script src="/js/lib/ejs.js"></script>
<% include layouts/headClose %>
<% include layouts/internalHeader %>
<% include layouts/containerFluidOpen %>
<%if(!createOrEdit){%>
<% include layouts/sidebar %>
<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
<% } %>
	<%if(typeof error_message !== 'undefined'){%>
	<p class='bg-error'><%= error_message%></p>
	<%} else if(typeof success_message !== 'undefined') { %>
	<p class='bg-info'><%= success_message%></p>
	<%} %>

<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">BakBak List<a href="/application/bakbaks/create" class="pull-right"><span class="glyphicon glyphicon-plus"></span></a></h3>
  </div>
  <div class="panel-body">
    <ul>
      <% for(var i=0; i < application.bakbaks.length;i++) { %>
      <li><a href="/application/bakbaks/create?id=<%=application.bakbaks[i]._id %>" ><%=application.bakbaks[i].name %></a></li>
      <% } %>
    <ul>
	</div>
</div>
<% include layouts/containerFluidClose %>
<% include layouts/footer %>
